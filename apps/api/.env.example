DATABASE_URL="postgresql://postgres:postgres@localhost:5435/scraper_db"
REDIS_HOST=localhost
REDIS_PORT=6380
PORT=3000
NODE_ENV=development

# Data Processor Configuration
DATA_PROCESSOR_BATCH_SIZE=50
DATA_PROCESSOR_BATCH_TIMEOUT_MS=5000
DATA_PROCESSOR_MAX_RETRIES=3
DATA_PROCESSOR_RETRY_DELAY_MS=1000
DATA_PROCESSOR_ENABLE_DEDUPLICATION=true
DATA_PROCESSOR_ENABLE_TRANSFORMATION=true
DATA_PROCESSOR_STORE_RAW_HTML=true
DATA_PROCESSOR_STORE_NORMALIZED_DATA=true
DATA_PROCESSOR_COMPRESSION_ENABLED=false

# Batch Processor Queue Configuration
BATCH_PROCESSOR_MAX_BATCH_SIZE=100
BATCH_PROCESSOR_MAX_QUEUE_SIZE=1000
BATCH_PROCESSOR_DEFAULT_PRIORITY=0

# Browser Configuration
HEADLESS_BROWSER=true
BROWSER_TIMEOUT=30000

HEADLESS_BROWSER=false
BROWSER_TIMEOUT=60000
BROWSER_DEVTOOLS=true
BROWSER_SLOWMO_MS=100
BROWSER_CHANNEL=chrome